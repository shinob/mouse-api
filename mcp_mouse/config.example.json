{
  "defaultServer": "local",
  "servers": {
    "local": {
      "baseUrl": "http://localhost:5000",
      "headers": {
        "X-API-Key": "your-api-key-here"
      }
    },
    "ipv6": {
      "baseUrl": "http://[::1]:5000", 
      "headers": {
        "X-API-Key": "your-api-key-here"
      }
    },
    "remote": {
      "baseUrl": "http://192.168.1.100:5000",
      "headers": {
        "X-API-Key": "your-api-key-here"
      }
    }
  }
}

